{
    "name": "sadashiv/lara-installer",
    "description": "Lara installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Sadashiv Mitra",
            "email": "qhstech@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Sadashiv\\LaraInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sadashiv\\LaraInstaller\\Providers\\LaraInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}